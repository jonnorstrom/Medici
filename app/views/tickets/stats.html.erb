<table>
  <tr>
    <th>
      Total Users
    </th>
  </tr>

  <tr>
    <td>
      <%= User.all.count %>
    </td>
  </tr>
</table>

<table>
  <tr>
    <th>
      Name
    </th>
    <th>
      Email
    </th>
    <th>
      Quantity
    </th>
  </tr>
  <% @tickets.each do |ticket| %>
  <tr>
      <td>
        <%= ticket.user.name %>
      </td>
      <td>
        <%= ticket.user.email %>
      </td>
      <td>
        <%= ticket.quantity %>
      </td>
  </tr>
  <% end %>

</table>
